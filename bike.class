public abstract class bike()
{
    //attributes for bike class
    String manufacturer;
    String modelYear;
    //constructor chain for bike class with information given
    public bike(){
    }

    public bike(String manufacturer){
        this.manufacturer = manufacturer;
    }

    public bike(String manufacturer, String modelYear){
        this.manufacturer = manufacturer;
        this.modelYear = modelYear;
    }

    void setManufacturer(String manufacturer){
        this.manufacturer = manufacturer;
    }

    String getManufacturer(){
        return this.manufacturer;
    }

    void setModelYear(String modelYear){
        this.modelYear = modelYear;
    }

    String getModelYear(){
        return this.modelYear
    }
}